{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#111111",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#E1E1E1",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#161616",
					"name": "Accent 1",
					"slug": "accent-1"
				},
				{
					"color": "#3D3D3D",
					"name": "Accent 2",
					"slug": "accent-2"
				},
				{
					"color": "#494949",
					"name": "Border Color",
					"slug": "accent-border"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "10px",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "20px",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "30px",
					"slug": "40"
				},
				{
					"name": "Regular",
					"size": "clamp(30px, 5vw, 50px)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "clamp(30px, 7vw, 70px)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "clamp(50px, 7vw, 90px)",
					"slug": "70"
				},
				{
					"name": "XX-Large",
					"size": "clamp(70px, 10vw, 140px)",
					"slug": "80"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/inter/Inter-VariableFont.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "italic",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/inter/Inter-Italic-VariableFont.woff2"
							]
						}
					],
					"fontFamily": "Inter, sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Outfit",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/outfit/Outfit-VariableFont.woff2"
							]
						}
					],
					"fontFamily": "Outfit, sans-serif",
					"name": "Outfit",
					"slug": "outfit"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Fira Code",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/fira-code/FiraCode-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "Fira Code, monospace",
					"name": "Fira Code",
					"slug": "fira-code"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.38rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.75rem"
					},
					"name": "Extra Large",
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.15rem"
					},
					"name": "Extra Extra Large",
					"size": "2.15rem",
					"slug": "xx-large"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/avatar": {
				"border": {
					"radius": "100%"
				}
			},
			"core/code": {
				"border": {
					"radius": "0.5rem"
				},
				"color": {
					"background": "var(--wp--preset--color--accent-2)"
				},
				"spacing": {
					"padding": {
						"bottom": "1rem",
						"left": "1rem",
						"right": "1rem",
						"top": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fira-code)",
					"fontWeight": "400"
				}
			},
			"core/comment-date": {
				"css": "& time { opacity: 0.65; }"
			},
			"core/comment-edit-link": {
				"border": {
					"color": "var(--wp--preset--color--accent-border)",
					"radius": "0.25rem",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--accent-2)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "4px",
						"left": "12px",
						"right": "12px",
						"top": "4px"
					}
				}
			},
			"core/comment-reply-link": {
				"border": {
					"color": "var(--wp--preset--color--accent-border)",
					"radius": "0.25rem",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--accent-2)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "4px",
						"left": "12px",
						"right": "12px",
						"top": "4px"
					}
				}
			},
			"core/comments-title": {
				"typography": {
					"fontWeight": "500"
				}
			},
			"core/image": {
				"border": {
					"radius": "0.5rem"
				}
			},
			"core/navigation": {
				"css": "> .wp-block-navigation__responsive-container { background: var(--wp--preset--color--base) !important; color: var(--wp--preset--color--contrast) !important;  }",
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--outfit)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"core/post-author": {
				"css": "& .wp-block-post-author__avatar { margin-right: 0.5rem; } & img { border-radius: 100%; }",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"css": "& p { font-size: var(--wp--preset--font-size--small); } & textarea, input { background: var(--wp--preset--color--accent-1); color: var(--wp--preset--color--contrast); border-radius: 0.5rem; border-color: var(--wp--preset--color--accent-border) !important; } & .wp-block-button input[type=submit] { background: var(--wp--preset--color--accent-2); }",
				"elements": {
					"heading": {
						"typography": {
							"fontWeight": "500"
						}
					}
				}
			},
			"core/post-date": {
				"css": "& time { white-space: nowrap; }",
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-excerpt": {
				"css": "& .wp-block-post-excerpt__more-link { background: var(--wp--preset--color--accent-2); color: var(--wp--preset--color--contrast); border: 1px solid var(--wp--preset--color--accent-border); padding: 4px 8px; border-radius: 8px; font-size: var(--wp--preset--font-size--small); }",
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "1rem"
				}
			},
			"core/post-terms": {
				"border": {
					"color": "var(--wp--preset--color--accent-border)",
					"radius": "1rem",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--accent-1)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "8px",
						"left": "16px",
						"right": "16px",
						"top": "8px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--outfit)",
					"fontWeight": "400"
				}
			},
			"core/query-pagination-numbers": {
				"css": "& .page-numbers & a, .page-numbers.current { padding: 0.25rem 0.6rem; margin: 8px; border-radius: 0.5rem; border: 1px solid var(--wp--preset--color--accent-border); background: var(--wp--preset--color--accent-2); } & .page-numbers.current { background: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); } & a { text-decoration: none !important; }"
			},
			"core/query-title": {
				"typography": {
					"fontWeight": "500"
				}
			},
			"core/search": {
				"css": "& .wp-block-search__input, .wp-block-search__button { border-radius: 100px; background: var(--wp--preset--color--accent-1); color: var(--wp--preset--color--contrast); border: 1px solid var(--wp--preset--color--accent-border); padding: 0.5rem 1rem }",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--outfit)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"core/video": {
				"css": "& video { border-radius: 0.5rem; }"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "currentColor"
				}
			}
		},
		"spacing": {
			"blockGap": "1.2rem",
			"padding": {
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "300",
			"letterSpacing": "-0.1px",
			"lineHeight": "1.4"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}
